#include <iostream>
using namespace std;

class rectangle{
    int length;
    int bredth;
    public:
    rectangle(){
        length= 1;
        bredth=1;
    }
    rectangle(int l,int b){
        length=l;
        bredth=b;
    }
    int area (){
        int ar = length * bredth;
        return ar;
    }
   
};

int main()
{
 rectangle r1;
 rectangle r2(2,4);
 cout<<r1.area()<<"\n";
 cout<<r2.area();
 

    return 0;
}
